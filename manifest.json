{
  "manifest_version": 2,
  "name": "github-zen-chrome",
  "description": "This extension shows a random github zen text on new tabs.",
  "version": "1.1",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://api.github.com/zen"
  ],
  "chrome_url_overrides" : {
    "newtab": "zen.html"
  }
}
